{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Graylog",
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "graylog",
          "port": "12201",
          "transportType": "Udp",
          "facility": "CatalogService"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "Application": "CatalogService"
    }
  }
}